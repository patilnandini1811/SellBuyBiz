{
  "compilerOptions": {
    "target": "ES2017",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    
    "noImplicitAny": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
   "baseUrl": "src",
"paths": {
  "@/*": ["./*"]
}

  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts", ".next/types/app/auth/welcome/page.tsx", "src/components/ui/CompanyList.tsx "],
  "exclude": ["node_modules"]
}
